@model IEnumerable<BlogWithDb.Models.PostsModel>
@{
    ViewBag.Title = "Index";
}
<div class="text-center">
    <h1><b>BLOG</b><br/></h1>
    <hr />
</div>  

<div>

</div>

<table class="table table-bordered table-responsive table-hover">  
    <tr>  
        <th>Time </th>  
        <th></th>  
     
    </tr>  
    @foreach (var d in Model)  
    {  
        <tr>  
            <td>@d.Text</td>  
            <td>@d.Title</td>  
            <td>@d.BlogId</td>   
        </tr>  
    }  
</table>  